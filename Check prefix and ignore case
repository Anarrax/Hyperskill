import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String str = sc.nextLine().toUpperCase(); // Example word is Java, then Kotlin. Sets string to upper case
        System.out.println(str.charAt(0) == 'J'); // Checks character at index 0 is 'J', prints Boolean result
        // Java will print true, Kotlin will print false
    }
}
