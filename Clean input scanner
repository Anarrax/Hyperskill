import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String name = sc.next(); // next() method finds and returns the next complete token from this scanner.
        sc.next(); // Does not declare a variable, not required for string output
        sc.next(); // Does not declare a variable, not required for string output
        sc.next(); // Does not declare a variable, not required for string output
        String cuisinePref = sc.next();
        // print used rather than println to keep as one printed sentence but split for better looking code
        System.out.print("The form for " + name + " is completed. We will contact you if we need a chef that cooks ");
        System.out.print(cuisinePref + " dishes.");
    }
}
